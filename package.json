    "@chromatic-com/storybook": "^4.1.1",
    "@markuplint/jsx-parser": "^4.7.21",
    "@markuplint/react-spec": "^4.5.21",
    "@pandacss/dev": "^1.4.2",
    "@storybook/addon-a11y": "^9.1.10",
    "@storybook/addon-docs": "^9.1.10",
    "@storybook/addon-mcp": "^0.0.5",
    "@storybook/addon-themes": "^9.1.10",
    "@storybook/addon-vitest": "^9.1.10",
    "@storybook/nextjs-vite": "^9.1.10",
    "@tsconfig/strictest": "^2.0.6",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "babel-plugin-react-compiler": "^1.0.0",
    "lefthook": "^1.13.6",
    "markuplint": "^4.13.1",
    "playwright": "^1.56.0",
    "storybook": "^9.1.10",
    "typescript": "^5",
    "vitest": "^3.2.4"
